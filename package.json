{
  "name": "@argue/vue-webrtc",
  "version": "1.3.0",
  "description": "WebRTC video component for Vue.js",
  "main": "dist/index.js",
  "author": {
    "name": "Gerardo Argueta",
    "email": "arguetagra15@gmail.com",
    "url": "https://github.com/argueta15"
  },
  "contributors": [],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.config.js",
    "builddemo": "cross-env NODE_ENV=production webpack --progress --hide-modules --config demo/webpack.config.js",
    "demo": "cross-env NODE_ENV=production webpack-dev-server --open --config demo/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --open --port 31415 --config demo/webpack.config.js",
    "devs": "cross-env NODE_ENV=development webpack-dev-server --hot --open --https --host 192.168.1.64 --port 31415 --config demo/webpack.config.js"
  },
  "dependencies": {
    "adapterjs": "^0.15.4",
    "rtcmulticonnection": "^3.6.9",
    "url-parse": "^1.4.7",
    "vue": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argueta15/vue-webrtc"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "function-bind": "^1.1.1",
    "html-webpack-plugin": "^4.0.2",
